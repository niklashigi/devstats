{
  "extends": ["tslint-xo/space"],
  "rules": {
    "quotemark": [true, "single", "avoid-escape"],
    "only-arrow-functions": false,
    "no-unused-expression": false,
    "no-require-imports": false,
    "curly": [true, "ignore-same-line"],
    "no-import-side-effect": false,
    "radix": false
  }
}
